[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = false
[init]
	defaultBranch = main
[core]
	autocrlf = input
	editor = vim
[push]
	autoSetupRemote = true
[credential]
	helper = store
